<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="gestaoProdutos">
	<class name="LinhaFactura" table="facturas_linhas">
		<id name="id" type="int" column="id">
			<generator class="increment" />
		</id>
		<property name="facturaId" type="int" column="facturas_id" />
		<property name="nome" type="String" column="nome" />
		<property name="morada" type="String" column="morada" />
		<property name="nif" type="String" column="nif" />
		<property name="empregadoId" type="int" column="empregados_id" />
	</class>
</hibernate-mapping>